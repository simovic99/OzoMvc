@model KategorijaPoslova

@{
    ViewData["Title"] = "Dodavanje kategorije posla";
}


<form asp-action="Create" method="post">
    <!--  <div asp-validation-summary="All"></div>
      <div class="form-group">
          <label asp-for="Id"></label>
          <input asp-for="Id" class="form-control"/>
      </div>-->
    <div class="form-group">
        <label asp-for="Naziv"></label>
        <div><span asp-validation-for="Naziv" class="text-danger"></span></div>
        <input asp-for="Naziv" class="form-control" />
    </div>

    <div class="text-centre">
        <button class="btn btn-primary" type="submit">Dodaj</button>
        <a asp-action="Index" class="btn btn-secondary">Odustani</a>
    </div>
</form>
@section styles{
    <link rel="stylesheet" href="~/lib/jqueryui/themes/base/jquery-ui.css" />
}

@section scripts{
    <script src="~/lib/jqueryui/jquery-ui.js"></script>
    <script src="~/lib/jqueryui/ui/i18n/datepicker-hr.js"></script>
    <script src="~/js/autocomplete.js"></script>


}